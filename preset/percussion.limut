// 909 style kick
preset bd pitchedperc,
	dur=1/4, // Default duration; 16th notes like a 909 sequencer
  // preset control params
  accent=this.value+this.add-3,
  tone=0,
  tune=pitch{4,oct:1},
  dec=1/3,
  // derived pitchedperc params
  hit=0, rattle=0,
	body={
  	2^(this.accent/8),
    freq:this.tune,
    wave:square,
    cutoff:1+(this.accent+this.tone)/15
  },
  click={
  	2^(this.accent/8),
    cutoff:1300+(this.accent+this.tone)*30,
  },
